<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="statistic Regression TestSuite ">
	<test name="create lead  test ">
		<groups>
			<run>
				<include name="createLead" />
			</run>
		</groups>
		<classes>
			<class name="com.affilomnia.leadbaskets.FormLbTest"></class>

		</classes>
	</test>
	<test name="buyer statistic check ">
		<groups>
			<run>
				<include name="buyerStatistic" />

			</run>
		</groups>
		<classes>
			<class name="com.affilomnia.leadbaskets.BuyerTest"></class>

		</classes>
	</test>
	<test name="seller statistic check ">
		<groups>
			<run>
				<include name="sellerStatistic" />
			</run>
		</groups>
		<classes>
			<class name="com.affilomnia.leadbaskets.SellerTest"></class>
		</classes>
	</test>
	<test name="admin seller statistic ">
		<groups>
			<run>
				<include name="adminStatistic" />
			</run>
		</groups>
		<classes>
			<class name="com.affilomnia.leadbaskets.AdminTest"></class>

		</classes>
	</test>

</suite>





